<odoo>
	<record id="view_partner_form" model="ir.ui.view">
		<field name="name">ol_graphql.view.partner.form</field>
		<field name="model">res.partner</field>
		<field name="inherit_id" ref="ol_base.view_partner_form"/>
		<field name="arch" type="xml">
			<page name="onlogic_info" position="inside">
				<group name="grp_graphql" string="GraphQL">
					<field name='origin_system' readonly="1"/>
					<field name='is_graphql_placeholder' readonly="1"/>
					<field name='last_graphql_queue_item_id' readonly="1"/>
					<field name='graphql_queue_ids'>
						<tree create="false" delete="false" editable="false" default_order="create_date desc">
							<field name="operation" readonly="1"/>
							<field name="state" readonly="1"/>
							<field name="transaction_id" readonly="1"/>
							<field name="publish_time" readonly="1"/>
							<field name="create_date" readonly="1"/>
						</tree>
					</field>
				</group>
			</page>
		</field>
	</record>
</odoo>