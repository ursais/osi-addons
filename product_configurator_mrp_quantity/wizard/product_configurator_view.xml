<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="product_configurator_form_inheriedt" model="ir.ui.view">
        <field name="name">product.configurator.product.template.form</field>
        <field name="model">product.configurator</field>
        <field name="inherit_id" ref="product_configurator.product_configurator_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='attribute_line_ids']//tree//field[@name='multi']"
                position="after"
            >
                <field name="is_qty_required" />
            </xpath>
        </field>
    </record>

</odoo>
