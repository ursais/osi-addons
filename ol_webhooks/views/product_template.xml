<odoo>
    <data>
        <record id="product_template_form_view" model="ir.ui.view">
            <field name="name">ol_webhooks.product.template.common.form.onlogic</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="ol_base.view_product_template_form"/>
            <field name="arch" type="xml">
                <page name="onlogic_info" position="inside">
                    <!-- Show the `system_stock_state` field for systems -->
                    <group name="webhooks">
                        <field name="has_configurable_attributes" readonly="1" invisible="1"/>
                    </group>
                </page>
            </field>
        </record>
    </data>
</odoo>