    <!-- Add net terms to company -->
    <record id="customer_company_us_property_payment_term_id" model="ir.property" forcecreate="True">
        <field name="name">property_payment_term_id</field>
        <field name="company_id" ref="base.main_company" />
        <field name="type">many2one</field>
        <field name="fields_id" search="[('model', '=', 'res.partner'),('name','=', 'property_payment_term_id')]" />
        <field name="res_id" eval="'res.partner,{}'.format(ref('ol_base.customer_company_us'))" />
        <field name="value_reference" eval="'account.payment.term,{}'.format(ref('account.account_payment_term_30days'))" />
    </record>

