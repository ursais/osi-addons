<odoo>

    <!-- web_m2x_options -->
    <record id="web_m2x_create" model="ir.config_parameter">
        <field name="key">web_m2x_options.create</field>
        <field name="value">False</field>
    </record>

    <record id="web_m2x_create_edit" model="ir.config_parameter">
        <field name="key">web_m2x_options.create_edit</field>
        <field name="value">False</field>
    </record>

    <record id="web_m2x_limit" model="ir.config_parameter">
        <field name="key">web_m2x_options.limit</field>
        <field name="value">15</field>
    </record>

    <record id="web_m2x_m2o_dialog" model="ir.config_parameter">
        <field name="key">web_m2x_options.m2o_dialog</field>
        <field name="value">False</field>
    </record>

</odoo>
