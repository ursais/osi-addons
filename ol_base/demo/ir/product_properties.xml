<odoo>

    <!-- ============================= -->
    <!-- Product Accounting Properties -->
    <!-- ============================= -->

    <!-- ================== -->
    <!-- Default Properties -->
    <!-- ================== -->

    <!-- Price Difference Account -->
    <record id="system_price_difference_account_default_us" model="ir.property">
        <field name="company_id" ref="base.main_company" />
        <field name="fields_id" search="[('model', '=', 'product.category'), ('name', '=', 'property_account_creditor_price_difference_categ')]" />
        <field name="name">property_account_creditor_price_difference_categ</field>
        <field name="value_reference"
               eval="'account.account,{}'.format(
                      ref('ol_base.{}_price_difference'.format(
                      ref('base.main_company'))))" />
    </record>

